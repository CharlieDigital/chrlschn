{
  "hosting": {
    "public": "dist",
    "headers": [
      {
        "source": "**/*.@(png|jpg|jpeg|webp|webm|mp4|gif|avif)",
        "headers": [
          {
            "key": "cache-control",
            "value": "max-age=26282880"
          }
        ]
      },
      {
        "source": "**/*.@(css|js)",
        "headers": [
          {
            "key": "cache-control",
            "value": "max-age=26282880"
          }
        ]
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
